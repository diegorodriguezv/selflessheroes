  "main": "src/electron.js",
  "scripts": {
    "start": "npm run serve:dev",
    "build-dev": "cross-env NODE_ENV=development webpack --env.platform=web --progress",
    "build": "cross-env NODE_ENV=production webpack --env.platform=web --progress",
    "serve:dev": "cross-env NODE_ENV=development webpack-dev-server --env.platform=web --progress --colors --host 0.0.0.0",
    "serve:prod": "cross-env NODE_ENV=production webpack-dev-server --env.platform=web --progress --colors --host 0.0.0.0",
    "build-electron": "cross-env NODE_ENV=production webpack --env.platform=electron --progress",
    "electron": "npm run build-electron & node ./build-electron.js",
    "electron:win64": "npm run build-electron & node ./build-electron.js win64",
    "electron:win32": "npm run build-electron & node ./build-electron.js win32",
    "electron:linux64": "npm run build-electron & node ./build-electron.js linux64",
    "electron:linux32": "npm run build-electron & node ./build-electron.js linux32",
    "electron:macos": "npm run build-electron & node ./build-electron.js macos"
  },
